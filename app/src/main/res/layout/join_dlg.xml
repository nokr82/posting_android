<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">

    <posting.devstories.com.posting_android.base.RationalRelativeLayout
        android:id="@+id/popup"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:ratio="960:515"
        android:background="@drawable/background_round_ffffff">

        <LinearLayout
            android:layout_marginLeft="44.43dp"
            android:layout_marginRight="44.43dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/messageTV"
                android:textSize="15.07dp"
                android:text="가입이 완료 되었습니다.\n\n캠퍼스생활 이제 떼고,붙이세요!"
                android:textColor="#6D6E71"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:gravity="center"/>

            <TextView
                android:id="@+id/PostingStartTX"
                android:layout_width="match_parent"
                android:layout_height="37.51dp"
                android:gravity="center"
                android:text="POSTING 시작하기"
                android:textSize="15.33dp"
                android:textColor="#20408C"
                android:background="#FEDA44"
                android:layout_marginBottom="16.5dp"
                />

        </LinearLayout>

    </posting.devstories.com.posting_android.base.RationalRelativeLayout>



</LinearLayout>